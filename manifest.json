{
  "manifest_version": 3,
  "name": "Fiverr Scraper",
  "version": "1.0",
  "description": "Downloads a tags upon user request.",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab", "scripting", "tabs", "downloads"],
  "host_permissions": ["https://www.fiverr.com/"],
  "background": {
    "service_worker": "src/background.js"
  }
}
